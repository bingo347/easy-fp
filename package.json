{
  "name": "lambda-fn",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "description": "Typescript utilities for functional programming inspired by Rust std library",
  "scripts": {
    "link-packages": "lerna link",
    "build": "rollup -c",
    "lint": "eslint --ext .ts --fix src",
    "test": "jest"
  },
  "packages": {
    "type-guards": {
      "version": "1.1.2"
    },
    "cell": {
      "version": "1.1.2",
      "additionalKeywords": [
        "svelte",
        "store"
      ],
      "dependencies": [
        "type-guards"
      ]
    },
    "option": {
      "version": "1.1.1",
      "additionalKeywords": [
        "maybe",
        "monad"
      ],
      "dependencies": [
        "type-guards"
      ]
    },
    "result": {
      "version": "1.1.0",
      "additionalKeywords": [
        "either",
        "monad"
      ],
      "dependencies": [
        "type-guards"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingo347/lambda-fn.git"
  },
  "author": "Dmitry Belyaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bingo347/lambda-fn/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.5",
    "lerna": "^4.0.0",
    "rollup": "^2.52.2",
    "rollup-plugin-ts": "^1.4.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}